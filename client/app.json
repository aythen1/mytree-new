{
  "expo": {
    "name": "MyTree",
    "slug": "mytreenueva",
    "version": "1.0.0",
    "scheme": "mytree",
    "orientation": "portrait",
    "icon": "./assets/myTreeLogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/myTreeLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.company.mytree"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "296deecb-db9a-47bc-9d00-4f8bd96480ba"
      }
    },
    "plugins": [
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      [
        "@react-native-google-signin/google-signin"
      ]
    ],
    "owner": "azschiaffino"
  }
}
